#include<iostream>
#include<cstdio>
#include<algorithm>
#include<string>
using namespace std;
struct slowo
{
    string s;
    int c;
    int w;
};
slowo *zlicz(slowo *s2) //CZEMU FUNKCJA ZWRACA WSKAÅ¹NIK DO STRUKTURY A NIE STRUKTURE?
{
int pom=0,pom2=0;
    int d = s2->s.size();
    for(int i=0;i<d;i++)
    {
        if(s2->s[i]>='A'&&s2->s[i]<='Z'||s2->s[i]>='a'&&s2->s[i]<='z')
        {
            pom++;
        }if(s2->s[i]>='0'&&s2->s[i]<='9')
        {
            pom2++;
        }
    }
    s2->w=pom;
    s2->c=pom2;
return s2;
}
int main()
{
 slowo s1;
 cin>>s1.s;
 zlicz(&s1);
 cout<<s1.w;
 cout<<s1.c;
}
