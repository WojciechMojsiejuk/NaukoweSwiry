#include<iostream>
#include<cstdio>
#include<algorithm>
using namespace std;
  struct punkt {
double x, y;
};
punkt *wyp(punkt *w,int n)
{
      for(int i=0;i<n;i++)
    {
        cin>>w[i].x>>w[i].y;
    }
    return w;


}
int main()
{
    int n;
    cout<<"ILE PUNKTÃ“W CHCESZ PODAC ?: ";
cin>> n;

 punkt *wsk= new punkt[n];
wyp(wsk,n);
for(int i=0;i<n;i++)
    {
        cout<<wsk[i].x<<" " <<wsk[i].y<<endl; //nie rozumiem czemu tutaj nie ma gwiazdek  bo przeciez odwoluje sie do wartosci a nie adresu
    }


}
